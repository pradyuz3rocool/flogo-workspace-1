{
  "name": "redis",
  "version": "0.0.1",
  "type": "flogo:activity",
  "description": "activity description",
  "ref": "github.com/shaliniGovindaNayak/flogo-workspace/activity/redis",
  "author": "ShaliniGovindaNayak <shalini.n@isteer.com>",
  "inputs":[
    {
      "name": "key",
      "type": "string"
    },
    {
      "name": "field",
      "type": "string"
    },
    {
      "name": "value",
      "type": "string"
    },
    {
      "name":"Key",
      "type":"string",
      "allowed":["TTL","Type","SetTTL","Dump","Del","DB","Exists","Flushall","FlushDB"]
    },
    {
      "name":"Strings",
      "type":"string",
      "allowed":["Set","Get","Increment","Decrement"]
    },
    {
      "name":"Hashing",
      "type":"string",
      "allowed":["hget","hset","hgetall","hkeys","hdel","hincrement"]
    },
    {
      "name":"Lists",
      "type":"string",
      "allowed":["List","Lpush","Lpop","Rpush","Rpop",""]
    },
    {
      "name":"Sets",
      "type":"string",
      "allowed":["Add","Members","Zadd","Zrem","Zscore","IsMember"]
    }
  ],
  "outputs": [
    {
      "name": "output",
      "type": "any"
    }
  ]
}