{
  "name": "redis",
  "version": "0.0.1",
  "type": "flogo:activity",
  "description": "activity description",
  "ref": "github.com/shaliniGovindaNayak/flogo-workspace/activity/redis",
  "author": "ShaliniGovindaNayak <shalini.n@isteer.com>",
  "inputs":[
    {
      "name": "key",
      "type": "string"
    },
    {
      "name": "field",
      "type": "string"
    },
    {
      "name": "value",
      "type": "string"
    },
    {
      "name": "select",
      "type":"string",
      "allowed": ["hash","list","strings"]
    },
    {
      "name":"hashing",
      "type":"string",
      "allowed":["hget","hset","hgetall","hkeys","hdel"]
    }
  ],
  "outputs": [
    {
      "name": "output",
      "type": "any"
    }
  ]
